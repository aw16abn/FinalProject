                  xmlns:detailviews="clr-namespace:FinalProject.Views.DetailViews"
                  x:Class="FinalProject.Views.Menu.MasterDetail">

    <MasterDetailPage.Master>
        <local:MasterPage x:Name="masterpage"/>
    </MasterDetailPage.Master>
    
    <MasterDetailPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <detailviews:InfoScreen1/>
            </x:Arguments>
               
            
        </NavigationPage>
    </MasterDetailPage.Detail>
    
</MasterDetailPage>